# Copy to .env.local for local development (DO NOT commit .env.local)
# Upstash REST credentials (create instance in https://upstash.com)
UPSTASH_REDIS_REST_URL="https://<your-id>.upstash.io"
UPSTASH_REDIS_REST_TOKEN="<your_upstash_token>"

# Optional: public poll id used by frontend
NEXT_PUBLIC_POLL_ID="ronda"

# Optional: allowed phones list (dev) - can be JSON array or newline-separated
# ALLOWED_PHONES='["081234567890","083846090672"]'
